<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="491.0" prefWidth="654.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.isp.studiomedico.controller.GestioneUtentiController">
    <children>
        <Label fx:id="benvenutoLabel" layoutX="236.0" layoutY="14.0" prefHeight="51.0" prefWidth="203.0" text="Gestione Utenti" textFill="#2196f3">
            <font>
                <Font size="18.0" />
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Label>

        <TableView fx:id="TurniTableView" layoutX="10.0" layoutY="120.0" prefHeight="250.0" prefWidth="633.0" style="-fx-background-color: #a0a2ab;">
            <columns>
                <TableColumn fx:id="utenteTableColumn" prefWidth="124.0" text="Utente" />
                <TableColumn fx:id="ruoloTableColumn" prefWidth="170.0" text="Ruolo" />
                <TableColumn fx:id="telefonoTableColumn" prefWidth="151.0" text="Telefono" />
                <TableColumn fx:id="emailTableColumn" prefWidth="187.0" text="Email" />
               
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
        <Label layoutX="214.0" layoutY="89.0" text="Elenco Utenti dello Studio Medico">
         <font>
            <Font size="15.0" />
         </font></Label>
      <Button fx:id="registraMedicoButton" layoutX="98.0" layoutY="404.0" mnemonicParsing="false" onAction="#registraMedico" text="Registra un Medico" />
      <Button fx:id="registraPazienteButton" layoutX="259.0" layoutY="404.0" mnemonicParsing="false" onAction="#registraPaziente" text="Registra un Paziente" />
      <Button fx:id="prenotazioneTelefonicaButton" layoutX="425.0" layoutY="404.0" mnemonicParsing="false" onAction="#prenotazioneTelefonica" prefHeight="25.0" prefWidth="148.0" text="Prenotazione Telefonica" />
    </children>
   <opaqueInsets>
      <Insets left="10.0" />
   </opaqueInsets>
</AnchorPane>
